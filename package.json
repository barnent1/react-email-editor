{
  "name": "unlayer-editor-react",
  "version": "2.0.0",
  "description": "Unlayer's Email Editor Component for React.js. UPDATED FOR React v17 and React Router v6",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
